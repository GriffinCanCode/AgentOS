{
  "name": "ai-os-ui",
  "version": "0.1.0",
  "type": "module",
  "description": "Dynamic UI renderer for AI-Powered OS",
  "main": "src/app/main.cjs",
  "scripts": {
    "dev": "concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,html}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,json,html}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "keywords": [
    "ai",
    "os",
    "electron",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@react-spring/web": "^10.0.3",
    "@tanstack/react-query": "^5.90.2",
    "@types/react-window": "^1.8.8",
    "@use-gesture/react": "^10.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "electron-log": "^5.4.3",
    "gsap": "^3.13.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.63.0",
    "react-rnd": "^10.5.2",
    "react-window": "^2.2.0",
    "web-vitals": "^4.2.4",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/d3": "^7.4.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "happy-dom": "^12.10.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.1.0",
    "wait-on": "^7.2.0"
  }
}
