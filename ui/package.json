{
  "name": "ai-os-ui",
  "version": "0.1.0",
  "type": "module",
  "description": "Dynamic UI renderer for AI-Powered OS",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently --kill-others --success first \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "npm run build:electron && wait-on http://localhost:5173 && electron .",
    "build": "npm run build:electron && vite build",
    "build:electron": "tsc -p electron/tsconfig.json",
    "build:electron:watch": "tsc -p electron/tsconfig.json --watch",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,html}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,json,html}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "keywords": [
    "ai",
    "os",
    "electron",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@floating-ui/react": "^0.27.16",
    "@floating-ui/react-dom": "^2.1.6",
    "@react-spring/web": "^10.0.3",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/react-window": "^1.8.8",
    "@use-gesture/react": "^10.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "colord": "^2.9.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "electron-log": "^5.4.3",
    "flexsearch": "^0.8.212",
    "fuse.js": "^7.1.0",
    "gsap": "^3.13.0",
    "lucide-react": "^0.294.0",
    "mathjs": "^14.8.2",
    "minisearch": "^7.2.0",
    "opentype.js": "^1.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.63.0",
    "react-rnd": "^10.5.2",
    "react-window": "^2.2.0",
    "reactflow": "^11.11.4",
    "recharts": "^3.2.1",
    "sonner": "^2.0.7",
    "string-similarity": "^4.0.4",
    "tinykeys": "^3.0.0",
    "ulid": "^3.0.1",
    "web-vitals": "^4.2.4",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/d3": "^7.4.3",
    "@types/opentype.js": "^1.3.8",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^8.2.2",
    "electron": "^38.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "happy-dom": "^19.0.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.3.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4",
    "wait-on": "^7.2.0"
  }
}
